
<%- include("header") -%>

    <div class="box" id="heading">
        <h1><%= ListTitle %></h1>
    </div>

    <div class="box">
            <% for (var i=0;i<newListItems.length;i++) { %>
                <div class="item">
                    <input type="checkbox">
                    <p> <%= newListItems[i] %> <p>
                </div>
            <% } %>


            <form class="item" action="/" method="POST">
                <input type="text" name="newItem" placeholder="New Item" autocomplete="off"><br>
                <button type="submit" name="key" value=<%= ListTitle %> >+</button>
            </form>
    </div>   

    <%- include("footer") -%>